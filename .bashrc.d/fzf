# Setup fzf
# ---------
if [[ ! "$PATH" == */home/eg.efremov/.fzf/bin* ]]; then
  PATH="${PATH:+${PATH}:}/home/eg.efremov/.fzf/bin"
fi

# Auto-completion
# ---------------
[[ $- == *i* ]] && source "/home/eg.efremov/.fzf/shell/completion.bash" 2> /dev/null

# Key bindings
# ------------
source "/home/eg.efremov/.fzf/shell/key-bindings.bash"
